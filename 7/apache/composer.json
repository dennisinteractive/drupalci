{
    "name": "drupal/core",
    "description": "Used to build D7 sites with composer",
    "type": "drupal-core",
    "version": "7",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "pdo": {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    },
    "require": {
        "composer/installers": "^1.6",
        "cweagans/composer-patches": "^1.6"
    },
    "enable-patching": true,
    "patches": {}
}
